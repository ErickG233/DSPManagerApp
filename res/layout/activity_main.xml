<?xml version="1.0" encoding="utf-8"?>

<com.bel.android.dspmanager.widgets.CustomDrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dsp_drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:animateLayoutChanges="true"
    tools:context=".activity.DSPManager">

    <FrameLayout
        android:id="@+id/dsp_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ListView
        android:id="@+id/dsp_navigation_drawer"
        android:background="@drawable/drawer_background"
        android:layout_width="260dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:choiceMode="singleChoice"
        android:divider="@android:color/transparent"
        android:dividerHeight="0dp" />

</com.bel.android.dspmanager.widgets.CustomDrawerLayout>

